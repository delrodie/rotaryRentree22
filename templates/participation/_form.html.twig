{{ form_start(form) }}
<div class="row form-group" data-aos="zoom-in" data-aos-easing="linear" data-aos-duration="3000">
    <div class="col-lg-6 mt-3">{{ form_row(form.nom) }}</div>
    <div class="col-lg-6 mt-3">{{ form_row(form.prenoms) }}</div>
    <div class="col-lg-6 mt-3">{{ form_row(form.email) }}</div>
    <div class="col-lg-6 mt-3">{{ form_row(form.contact) }}</div>
    <div class="col-lg-6 mt-3">{{ form_row(form.club) }}</div>
    <div class="col-lg-6 mt-3">{{ form_row(form.poste) }}</div>
    <div class="col-lg-6 mt-3">{{ form_row(form.nombrePlace) }}</div>
</div>
{{ form_widget(form) }}
<div class="col-lg-12 d-grid gap-2 mt-5 mb-3">
    <button class="btn btn-primary">{{ button_label|default('Envoyer') }}</button>
</div>
{{ form_end(form) }}
